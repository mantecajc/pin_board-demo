<div class="transitions-enabled" id="pins">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= image_tag pin.image %>
      <div class="panel-body">
        <h2>
          <%= link_to pin.title, pin %>
        </h2>
        <p class="user">Submitted by <%= pin.user.email %></p>  
      </div>
    </div>
  <% end %>
</div>